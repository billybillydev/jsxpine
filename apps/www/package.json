{
	"name": "www",
	"version": "1.14.9",
	"module": "src/index.js",
	"scripts": {
		"dev": "bun run --hot src/index.tsx",
		"tailwind:dev": "bunx tailwindcss -i ./src/styles/tailwind.css -o ./public/styles/index.css --watch",
		"tailwind:prod": "bunx tailwindcss -i ./src/styles/tailwind.css -o ./public/styles/index.css",
		"build:watch": "bunx esbuild --bundle --minify ./src/scripts/app.js --outfile=./public/script/app.js --watch",
		"build:prod": "bunx esbuild --bundle --minify ./src/scripts/app.js --outfile=./public/script/app.js",
		"start:dev": "concurrently \"bun dev\" \"bun tailwind:dev\" \"bun build:watch\"",
		"start:prod": "bun run src/index.tsx && bun tailwind:prod && bun build:prod",
		"build:common": "bun run build-scripts/common.js",
		"build:components": "bun run build-scripts/components.js",
		"build:alpine": "bun run build-scripts/alpine-dependencies.js",
		"build:registry": "bun run build-scripts/registry.js",
		"jsx:test": "xss-scan"
	},
	"dependencies": {
		"@alpinejs/collapse": "^3.12.3",
		"@alpinejs/focus": "^3.12.3",
		"@iconify-json/ri": "^1.1.12",
		"@iconify/utils": "^2.1.11",
		"@kitajs/html": "^4.1.0",
		"@kitajs/ts-html-plugin": "^4.0.1",
		"@t3-oss/env-core": "^0.9.2",
		"alpinejs": "^3.12.3",
		"alpinejs-manage": "^1.0.3",
		"change-case": "^5.4.4",
		"clsx": "^2.1.0",
		"colortranslator": "^4.1.0",
		"hono": "^4.2.3",
		"shikiji": "^0.10.2",
		"tailwindcss": "^3.0.24",
		"tailwindcss-mosiui-mini": "^0.1.4"
	},
	"devDependencies": {
		"@types/alpinejs": "^3.13.10",
		"@types/bun": "latest",
		"concurrently": "^8.2.0",
		"zod": "^3.22.4"
	}
}
